<div class="container">
	<h1>Choose an entity to search for</h1>

	<%= form_tag(semantic_search_path, :method => "post") do %>
	<%= select_tag("term", options_for_select(["legal-vocabulary", "time", "number", "person", "first-name", "surname", "relationship", "place", "general-office", "election"]), class:"form-control")%>
	<%= submit_tag "Search", class: "btn btn-primary" %>
	<% end %>
</div>